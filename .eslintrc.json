{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-new": "off",
        "import/prefer-default-export": "off",
        "max-len": ["warn", {
            "code": 120,
            "comments": 200
        }],
        "no-console": "warn"
    }
}
